{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "outputDirectory": "public",
  "buildCommand": "pnpm run tsup",
  "installCommand": "pnpm install",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api"
    }
  ],
  "redirects": [{ "source": "/", "destination": "/api", "permanent": false }],
  "functions": {
    "api/index.js": {
      "memory": 128,
      "maxDuration": 10
    }
  }
}
