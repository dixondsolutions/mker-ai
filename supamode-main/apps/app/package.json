{
  "name": "app",
  "private": true,
  "version": "0.1.26",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "clean": "git clean -xdf .next .turbo node_modules",
    "lint:fix": "eslint . --fix",
    "format": "prettier --check \"**/*.{js,cjs,mjs,ts,tsx,md,json}\"",
    "typecheck": "tsc --noEmit",
    "build": "vite build",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "preview": "vite preview --port 5173",
    "supabase": "supabase",
    "supabase:start": "supabase status || supabase start",
    "supabase:stop": "supabase stop",
    "supabase:reset": "supabase db reset",
    "supabase:status": "supabase status",
    "supabase:test": "supabase db test",
    "supabase:db:diff": "supabase db diff",
    "supabase:db:lint": "supabase db lint",
    "supabase:deploy": "supabase link --project-ref $SUPABASE_PROJECT_REF && supabase db push",
    "supabase:typegen": "pnpm run supabase:typegen:packages && pnpm run supabase:typegen:app",
    "supabase:typegen:packages": "supabase gen types typescript --local > ../../packages/supabase/src/database.types.ts",
    "supabase:typegen:app": "supabase gen types typescript --local > ./src/database.types.ts",
    "supabase:db:dump:local": "supabase db dump --local --data-only"
  },
  "dependencies": {
    "@tanstack/react-query": "catalog:",
    "i18next-browser-languagedetector": "catalog:",
    "i18next-resources-to-backend": "catalog:",
    "lucide-react": "catalog:",
    "next-themes": "catalog:",
    "react": "catalog:",
    "react-dom": "catalog:",
    "react-router": "catalog:",
    "rxjs": "catalog:",
    "tw-animate-css": "catalog:"
  },
  "devDependencies": {
    "@eslint/js": "catalog:",
    "@kit/audit-logs": "workspace:*",
    "@kit/auth": "workspace:*",
    "@kit/captcha": "workspace:*",
    "@kit/dashboards": "workspace:*",
    "@kit/data-explorer": "workspace:*",
    "@kit/navigation": "workspace:*",
    "@kit/prettier-config": "workspace:*",
    "@kit/settings": "workspace:*",
    "@kit/shared": "workspace:*",
    "@kit/storage-explorer": "workspace:*",
    "@kit/supabase": "workspace:*",
    "@kit/text-editor": "workspace:*",
    "@kit/typescript-config": "workspace:*",
    "@kit/ui": "workspace:*",
    "@kit/users-explorer": "workspace:*",
    "@tailwindcss/vite": "catalog:",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "@vitejs/plugin-react": "catalog:",
    "eslint": "catalog:",
    "eslint-plugin-react-hooks": "catalog:",
    "eslint-plugin-react-refresh": "catalog:",
    "supabase": "catalog:",
    "tailwindcss": "catalog:",
    "typescript": "catalog:",
    "typescript-eslint": "catalog:",
    "vite": "catalog:",
    "zod": "catalog:"
  },
  "prettier": "@kit/prettier-config"
}
