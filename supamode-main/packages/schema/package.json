  "dependencies": {
    "chalk": "catalog:",
    "commander": "catalog:"
  },
  "devDependencies": {
    "@kit/eslint-config": "0.1.0",
    "@kit/prettier-config": "0.1.0",
    "@kit/typescript-config": "0.1.0",
    "@supabase/supabase-js": "catalog:",
    "@types/node": "catalog:",
    "zod": "catalog:"
  },
  "prettier": "@kit/prettier-config",
  "typesVersions": {
    "*": {
      "*": [
        "src/*"
      ]
    }
  }
}
