{
  "$schema": "https://turbo.build/schema.json",
  "ui": "stream",
  "tasks": {
    "topo": {
      "dependsOn": ["^topo"]
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "dev": {
      "persistent": true,
      "cache": false
    },
    "format": {
      "outputs": ["node_modules/.cache/.prettiercache"],
      "outputLogs": "new-only"
    },
    "lint": {
      "dependsOn": ["^topo"],
      "outputs": ["node_modules/.cache/.eslintcache"]
    },
    "typecheck": {
      "dependsOn": ["^topo"],
      "outputs": ["node_modules/.cache/tsbuildinfo.json"]
    },
    "test": {
      "dependsOn": ["^topo"],
      "outputs": ["coverage/cover.xml"]
    },
    "clean": {
      "cache": false
    },
    "//#clean": {
      "cache": false
    },
    "license#dev": {
      "outputLogs": "errors-only"
    }
  },
  "globalEnv": ["SUPABASE_DATABASE_URL"]
}
